    <script>
        // الدالة المسؤولة عن إخفاء شاشة البداية
        function hideSplashScreen() {
            const splashScreen = document.getElementById('splash-screen');
            if (splashScreen) {
                // إضافة الفئة 'hidden' التي تحتوي على Opacity: 0 و Visibility: hidden
                splashScreen.classList.add('hidden');
            }

            // (اختياري) إزالة العنصر تمامًا من DOM بعد انتهاء الحركة (0.5 ثانية)
            setTimeout(() => {
                if (splashScreen) {
                    splashScreen.remove();
                }
            }, 500);
        }

        // تعيين مؤقت (Delay) لمدة 3000 ملي ثانية (3 ثواني) قبل استدعاء دالة الإخفاء
        window.onload = function() {
            setTimeout(hideSplashScreen, 3000); // 3000ms = 3 ثواني
        };
    </script>
